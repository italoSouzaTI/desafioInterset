{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "@assets/*": ["./src/assets/*"],
            "@core/*": ["./src/core/*"],
            "@features/*": ["./src/features/*"],
            "@hooks/*": ["./src/hooks/*"],
            "@provider/*": ["./src/provider/*"],
            "@routes/*": ["./src/routes/*"],
            "@shared/*": ["./src/shared/*"],
            "@store/*": ["./src/store/*"]
        }
    }
}
